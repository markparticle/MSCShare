<!--pages/classic/classic.wxml-->
<!-- 骨架文件 -->

<view class="header">
    <v-epsoide class="epsoide" index="{{classicData.index}}" content="{{classicData.content}}"></v-epsoide>
    <v-like class="like" bind:like="onLike" like="{{classicData.like_status}}" count="{{classicData.fav_nums}}"></v-like>
</view>
<v-movie img="{{classicData.image}}" content="{{classicData.content}}"></v-movie>